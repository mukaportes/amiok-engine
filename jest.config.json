{
  "testEnvironment": "node",
  "collectCoverage": true,
  "coverageThreshold": {
    "global": {
      "branches": 100,
      "functions": 100,
      "lines": 100,
      "statements": 100
    }
  },
  "collectCoverageFrom": ["src/**/*.js"],
  "testMatch": ["**/?(*.)(test).js?(x)"],
  "testPathIgnorePatterns": ["<rootDir>/node_modules", "<rootDir>/src"]
}
